{
  "root": true,
  "env": {
    "browser": true
  },
  "parserOptions": { "ecmaVersion": "latest", "sourseType": "Module" },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react/jsx/runtime",
    "plugin:jsx-ally/recommended",
    "prettier"
  ],
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "overrides": [{ "files": ["*.ts", "*.tsx"] }]
}
